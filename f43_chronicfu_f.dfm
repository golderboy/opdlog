  object Qf43_chronicfu: TMyQuery
    SQLInsert.Strings = (
      'INSERT INTO opdscreen'
      
        '  (vn, hn, vstdate, vsttime, bpd, bps, bw, cc, hr, pe, pulse, te' +
        'mperature, rr, height, bmi, hpi, pmh, waist)'
      'VALUES'
      
        '  (:vn, :hn, :vstdate, :vsttime, :bpd, :bps, :bw, :cc, :hr, :pe,' +
        ' :pulse, :temperature, :rr, :height, :bmi, :hpi, :pmh, :waist)')
    Connection = db_connect_m.connect_db
    SQL.Strings = (
      'SELECT'
      '(SELECT hospitalcode FROM opdconfig) AS HOSPCODE,'
      'IFNULL(p.person_id,pt.hn) AS PID,'
      'o.vn as SEQ,'
      
        'IF(c.screen_date IS NULL OR TRIM(c.screen_date)='#39#39' OR c.screen_d' +
        'ate LIKE '#39'0000-00-00%'#39','#39#39',DATE_FORMAT(c.screen_date,'#39'%Y%m%d'#39')) A' +
        'S DATE_SERV,'
      
        'IF(o.bw IS NOT NULL AND TRIM(o.bw )<>'#39#39', REPLACE(FORMAT(o.bw ,2)' +
        ','#39','#39','#39#39'), FORMAT(0,2)) AS WEIGHT,'
      
        'IF(o.height IS NOT NULL AND TRIM(o.height )<>'#39#39', REPLACE(FORMAT(' +
        'o.height ,2),'#39','#39','#39#39'), FORMAT(0,2)) AS HEIGHT,'
      'o.waist AS WAIST_CM,'
      
        'IF(o.bps IS NOT NULL AND TRIM(o.bps )<>'#39#39', REPLACE(FORMAT(o.bps ' +
        ',0),'#39','#39','#39#39'), FORMAT(0,0)) AS SBP,'
      
        'IF(o.bpd IS NOT NULL AND TRIM(o.bpd )<>'#39#39', REPLACE(FORMAT(o.bpd ' +
        ',0),'#39','#39','#39#39'), FORMAT(0,0)) AS DBP,'
      
        '(SELECT CASE (CONCAT(cfs.dmht_foot_scr_res_left_id,cfs.dmht_foot' +
        '_scr_res_right_id)) '
      'WHEN '#39'00'#39' THEN '#39'2'#39
      'WHEN '#39'11'#39' THEN '#39'1'#39
      'WHEN '#39'12'#39' THEN '#39'3'#39
      'WHEN '#39'13'#39' THEN '#39'3'#39
      'WHEN '#39'14'#39' THEN '#39'3'#39
      'WHEN '#39'21'#39' THEN '#39'3'#39
